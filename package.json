{
  "name": "deb.ug",
  "version": "1.0.0",
  "description": "Personal website for Brian Gaffney",
  "main": "src/main.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --progress",
    "build": "npm run deleteBuild && NODE_ENV=production webpack --config webpack.dist.config.js --colors --progress --display-chunks --display-modules",
    "deleteBuild": "rm -rf ./build",
    "deploy": "npm run build && ./deploy.sh && npm run deleteBuild",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Brian-Gaffney/Deb.ug.git"
  },
  "author": "Brian Gaffney",
  "bugs": {
    "url": "https://github.com/Brian-Gaffney/Deb.ug/issues"
  },
  "homepage": "https://deb.ug",
  "dependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel": "^6.3.13",
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "bluebird": "^3.3.1",
    "eslint-friendly-formatter": "^1.2.2",
    "file-loader": "^0.8.5",
    "hex-rgb": "^1.0.0",
    "html-minifier": "^1.2.0",
    "html-webpack-plugin": "^1.7.0",
    "path": "^0.12.7",
    "radium": "^0.18.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-google-analytics": "^0.2.0",
    "react-hot-loader": "^1.3.0",
    "resolve-url-loader": "^1.4.3",
    "svg-jsx-loader": "0.0.11",
    "three-js": "^74.0.1",
    "tween.js": "^16.3.4",
