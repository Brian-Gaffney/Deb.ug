{
  "name": "deb.ug",
  "version": "1.0.0",
  "description": "Personal website for Brian Gaffney",
  "main": "src/main.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --progress",
    "build": "npm run deleteBuild && NODE_ENV=production webpack --config webpack.dist.config.babel.js --colors --progress --display-chunks --display-modules",
    "deleteBuild": "rm -rf ./build",
    "deploy": "npm run build && ./deploy.sh && npm run deleteBuild",
    "lint": "./node_modules/.bin/eslint --ext .js ./",
    "lint-fix": "./node_modules/.bin/eslint --fix --ext .js ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Brian-Gaffney/Deb.ug.git"
  },
  "author": "Brian Gaffney",
  "bugs": {
    "url": "https://github.com/Brian-Gaffney/Deb.ug/issues"
  },
  "homepage": "https://deb.ug",
  "dependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel": "^6.5.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "bluebird": "^3.3.1",
    "eslint": "^3.15.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.9.0",
    "favicolor": "^1.0.1",
    "file-loader": "^0.10.0",
    "html-minifier": "^1.2.0",
    "html-webpack-plugin": "^2.28.0",
    "jss": "^6.2.0",
    "jss-nested": "^3.0.1",
    "path": "^0.12.7",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-google-analytics": "^0.2.0",
    "react-helmet": "^4.0.0",
    "react-hot-loader": "^1.3.1",
    "react-jss": "^5.3.0",
    "resolve-url-loader": "^1.6.1",
    "three-js": "^79.0.0",
    "tween.js": "^16.3.4",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}
